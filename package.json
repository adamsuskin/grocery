{
  "name": "grocery-list",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "zero:dev": "zero-cache-dev",
    "db:up": "docker compose up -d",
    "db:down": "docker compose down",
    "dev:full": "concurrently \"pnpm db:up\" \"pnpm zero:dev\" \"pnpm dev\""
  },
  "dependencies": {
    "@rocicorp/zero": "^0.1.0",
    "kysely": "^0.27.3",
    "nanoid": "^5.0.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^8.2.2",
    "typescript": "^5.5.3",
    "vite": "^5.3.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": ["@rocicorp/zero-sqlite3"]
  }
}
